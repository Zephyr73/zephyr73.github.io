  "type": "module",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npx @11ty/eleventy",
    "build:css": "sass src/scss/style.scss src/assets/css/style.css --no-source-map",
    "watch:css": "sass --watch src/scss/style.scss src/assets/css/style.css",
    "dev": "concurrently \"npm:start\" \"npm:watch:css\"",
    "format": "prettier --write \"**/*.{html,js,css,scss,md}\"",
    "lint:js": "eslint \"**/*.js\"",
    "lint": "npm run lint:js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx @11ty/eleventy --serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@eslint/js": "^9.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.0.0",
    "exif-reader": "^2.0.3",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3"
  }
